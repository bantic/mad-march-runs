<div class="selected-teams">
  {{#each teamSlots as |teamSlot|}}
    {{#if teamSlot.team}}
      <div class="team">
        SELECTED:
        <div class="name">{{team.name}}</div>
        <div class="seed">{{team.seed}}</div>
      </div>
    {{else}}
      <div class='team'>empty</div>
    {{/if}}
  {{/each}}
</div>

<div class="teams">
  {{#each model as |team|}}
  <div class="team">
    <div class="name">{{team.name}} </div>
    <div class='seed'>{{team.seed}}</div>
  </div>
  {{/each}}
</div>
